<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <import resource="../common.xml"/>
    <import resource="pmc-processor.xml"/>
    <!-- included for catalogServiceURI, etc. -->
    <import resource="../lane/marc-processor.xml"/>
    <bean id="solrLoader" class="edu.stanford.irt.eresources.PmcReload" parent="abstractSolrLoader">
        <property name="processors">
            <list>
                <ref bean="pmcProcessor"/>
            </list>
        </property>
    </bean>
</beans>
