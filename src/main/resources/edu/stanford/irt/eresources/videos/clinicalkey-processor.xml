<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"     xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">
    <bean id="clinicalkeyProcessor"
        class="edu.stanford.irt.eresources.sax.videos.clinicalkey.ClinicalkeyEresourceProcessor">
        <property name="contentHandler" ref="clinicalkeyBuilder"/>
        <property name="URLs">
        	<list>
        		<value>https://www.clinicalkey.com/service/v2/auth/ip-address</value>
        		<value>https://www.clinicalkey.com/service/browse/multimedia?ckproduct=CK_US&amp;contenttype=MM&amp;facetquery=contenttype:%22VD%22&amp;onlyEntitled=true&amp;product=en_US&amp;rows=100&amp;start=0</value>
       		</list>
       	</property>
     </bean>
   
    <bean id="clinicalkeyBuilder" class="edu.stanford.irt.eresources.sax.DefaultEresourceBuilder">
        <property name="eresourceHandler" ref="eresourceHandler"/>
    </bean>
    
     <bean id="solrLoader" class="edu.stanford.irt.eresources.SolrVideosReload" parent="abstractSolrLoader">
    		<property name="processors">
			<list>
				 <ref bean="clinicalkeyProcessor" /> 
			</list>
        </property>
    </bean>
    
    <import resource="video-reload.xml"/>
    
</beans>