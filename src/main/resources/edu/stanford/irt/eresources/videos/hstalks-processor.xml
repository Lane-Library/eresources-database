<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"     xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">
    <bean id="hstalksProcessor"
        class="edu.stanford.irt.eresources.sax.videos.hstalks.HstalksEresourceProcessor">
        <property name="contentHandler" ref="hstalksBuilder"/>
        <property name="URLs">
        	<list>
        		<value>https://hstalks.com/search/ajax/results/?biosci=&amp;subtype=TALK&amp;offset=</value>
       		</list>
       	</property>
     </bean>
   
    <bean id="hstalksBuilder" class="edu.stanford.irt.eresources.sax.DefaultEresourceBuilder">
        <property name="eresourceHandler" ref="eresourceHandler"/>
    </bean>
</beans>