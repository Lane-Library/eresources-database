<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">
	<bean id="accessmedicineProcessor" class="edu.stanford.irt.eresources.sax.videos.accessmedicine.AccessmedicineEresourceProcessor">
		<property name="contentHandler" ref="accessmedicineTransformer" />

	</bean>
	<bean id="accessmedicineTransformer" class="edu.stanford.lane.catalog.impl.xml.FilteringXSLTTransformer"
		init-method="init">
		<property name="stylesheet" value="classpath://edu/stanford/irt/eresources/videos/accessmedicine2er.xsl" />
		<property name="transformerFactory" value="net.sf.saxon.TransformerFactoryImpl" />
		<property name="namespace" value="" />
		<property name="element" value="eresources" />
		<property name="contentHandler" ref="accessmedicineBuilder" />
	</bean>
	<bean id="accessmedicineBuilder" class="edu.stanford.irt.eresources.sax.DefaultEresourceBuilder">
		<property name="eresourceHandler" ref="eresourceHandler" />
	</bean>

	<bean id="solrLoader" class="edu.stanford.irt.eresources.SolrVideosReload" parent="abstractSolrLoader">
		<property name="processors">
			<list>
				<ref bean="accessmedicineProcessor" />
			</list>
		</property>
	</bean>

	<import resource="video-reload.xml" />
</beans>
