<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">
    <bean id="printProcessor" class="edu.stanford.irt.eresources.marc.MarcPrintProcessor">
        <constructor-arg ref="eresourceHandler"/>
        <constructor-arg>
            <bean class="org.marc4j.MarcStreamReader">
                <constructor-arg ref="printBibInputStream"/>
            </bean>
        </constructor-arg>
        <constructor-arg ref="itemCount"/>
        <constructor-arg ref="keywordsStrategy"/>
    </bean>
</beans>
