<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">
    <bean id="classesProcessor" class="edu.stanford.irt.eresources.ETLProcessor">
        <constructor-arg>
            <bean class="edu.stanford.irt.eresources.classes.ClassesExtractor">
                <constructor-arg value="${eresources.lane-host}/classes/all-data.xml"/>
                <constructor-arg ref="startDate"/>
            </bean>
        </constructor-arg>
        <constructor-arg>
            <bean class="edu.stanford.irt.eresources.classes.ClassesTransformer">
                <constructor-arg value="classpath:/edu/stanford/irt/eresources/classes2er.xsl"/>
                <constructor-arg value="${eresources.lane-host}"/>
            </bean>
        </constructor-arg>
        <constructor-arg ref="loader"/>
    </bean>
</beans>
