<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">
    <bean id="selectProcessor" class="edu.stanford.irt.eresources.sax.MARCEresourceProcessor">
        <property name="inputStream" ref="selectBibInputStream"/>
        <property name="xmlReader" >
            <bean class="edu.stanford.lane.catalog.impl.xml.UTF8ComposingMarcReader">
                <property name="contentHandler">
                    <bean  class="edu.stanford.irt.eresources.sax.PrintMARCEresourceBuilder">
                        <property name="eresourceHandler" ref="eresourceHandler"/>
                        <property name="authTextAugmentation" ref="authTextAugmentation"/>
                        <property name="itemCount" ref="itemCount"/>
                    </bean>
                </property>
            </bean>
        </property>
    </bean>
</beans>
