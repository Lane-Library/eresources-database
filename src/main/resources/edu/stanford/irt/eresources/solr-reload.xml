<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">
    <import resource="solr-load.xml"/>
    <bean id="solrLoader" class="edu.stanford.irt.eresources.SolrReload" parent="abstractSolrLoader">
    	<property name="processors">
            <list>
                <ref bean="bibProcessor"/>
                <ref bean="webProcessor"/>
                <ref bean="classesProcessor"/>
                <ref bean="laneblogProcessor"/>
            </list>
        </property>
    </bean>
    <bean id="catalogRecordService" class="edu.stanford.irt.eresources.HTTPCatalogRecordService">
        <constructor-arg ref="catalogServiceURI"/>
    </bean>
</beans>
