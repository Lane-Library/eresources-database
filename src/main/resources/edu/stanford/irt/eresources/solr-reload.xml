<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">
    <import resource="solr-load.xml"/>
    <bean id="solrLoader" class="edu.stanford.irt.eresources.SolrReload" parent="abstractSolrLoader">
        <property name="optimize" value="false"/>
    </bean>
    <bean id="bibInputStream" class="edu.stanford.irt.eresources.BibInputStream">
        <property name="dataSource" ref="voyagerDataSource"/>
        <property name="executor" ref="executor"/>
    </bean>
    <bean id="printBibInputStream" class="edu.stanford.irt.eresources.PrintBibInputStream">
        <property name="dataSource" ref="voyagerDataSource"/>
        <property name="executor" ref="executor"/>
    </bean>
</beans>
